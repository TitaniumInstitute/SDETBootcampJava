<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite IText">
    <test name="Group Smoke Test">
        <groups>
            <run>
                <include name="Smoke"/>
            </run>
        </groups>
        <classes>
            <class name="com.ti.testng.dataprovider.ItestContextExample"></class>
        </classes>
    </test>

    <test name="Group Regression Test">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>
        <classes>
            <class name="com.ti.testng.dataprovider.ItestContextExample"></class>
        </classes>
    </test>
</suite>